{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "continue": true
      },
      {
        "src": "/.*\\.(js|css|ico|png|jpg|jpeg|svg|woff|woff2|ttf|eot)$",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }